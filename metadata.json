{
  "name": "puppetfinland-xtrabackup",
  "version": "1.0.2",
  "author": "Puppet-Finland team",
  "summary": "Manage xtrabackup-based backups",
  "license": "BSD-2-Clause",
  "source": "https://github.com/Puppet-Finland/puppet-xtrabackup.git",
